{
  "displayName": "Hugging Face IMDB Dataset",
  "name": "hf-imdb-dataset",
  "description": "IMDB dataset from Hugging Face with 500 text labeled with pos or neg",
  "scope": "public",
  "version": "0.0.12",
  "codebase": {
    "type": "git",
    "gitUrl": "https://github.com/dataloop-ai-apps/datasets-huggingface.git",
    "gitTag": "0.0.12"
  },
  "attributes": {
    "Provider": "Hugging Face",
    "Category": "Dataset",
    "License": "CC BY 4.0"
  },
  "components": {
    "computeConfigs": [
      {
        "name": "huggingface-deploy",
        "secrets": [],
        "runtime": {
          "podType": "regular-m",
          "concurrency": 1,
          "runnerImage": "gcr.io/viewo-g/piper/agent/cpu/huggingface_datasets:0.0.2",
          "autoscaler": {
            "type": "rabbitmq",
            "minReplicas": 0,
            "maxReplicas": 2,
            "queueLength": 1000
          },
          "preemptible": false
        }
      }
    ],
    "modules": [
      {
        "name": "dataset-adapter",
        "entryPoint": "dataset_loader.py",
        "className": "DatasetHF",
        "description": "Dataset adapter for Hugging Face",
        "initInputs": [],
        "functions": [
          {
            "name": "upload_dataset_text",
            "input": [
              {
                "type": "Dataset",
                "name": "dataset",
                "description": "Dataloop Dataset Entity"
              },
              {
                "type": "String",
                "name": "source",
                "description": "Dataset source URL"
              }
            ],
            "output": [],
            "displayName": "upload_dataset_text",
            "displayIcon": "",
            "description": "function to upload dataset to Dataloop"
          }
        ]
      }
    ],
    "datasets": [
      {
        "name": "huggingface-imdb",
        "docs": "http://ai.stanford.edu/~amaas/data/sentiment/",
        "source": "https://huggingface.co/datasets/stanfordnlp/imdb",
        "invoke": {
          "namespace": "dataset-adapter.upload_dataset_text"
        },
        "metadata": {
          "system": {
            "importConfig": {
              "type": "text_classification",
              "id_to_label_map": {
                "0": "neg",
                "1": "pos"
              }
            }
          }
        },
        "computeConfig": "huggingface-deploy"
      }
    ]
  }
}
